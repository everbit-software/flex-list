{
    "compilerOptions": {
        "target": "ESNext",
        "outDir": "./dist/",
        "rootDir": "./src",
        "declaration": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node"
    },
    "include": [
        "./src/**/*.ts"
    ],
}